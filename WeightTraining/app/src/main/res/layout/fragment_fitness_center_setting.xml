<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".management.project.fragment.More.FitnessCenterSettingFragment">

    <include
        layout="@layout/include_top_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_default"
        android:background="@color/colorDividerDefault" />

    <Switch
        android:id="@+id/f_fitness_center_setting_is_allowed_access_notification"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/eland_choice_medium"
        android:padding="20dp"
        android:text="@string/f_fitness_center_setting_is_allowed_access_notification"
        android:textSize="@dimen/text_sub_title_size" />

    <include
        layout="@layout/include_dot_divider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <Switch
        android:id="@+id/f_fitness_center_setting_is_disclosed"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/eland_choice_medium"
        android:padding="20dp"
        android:text="@string/f_fitness_center_setting_is_disclosed"
        android:textSize="@dimen/text_sub_title_size" />

    <include
        layout="@layout/include_dot_divider"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>



</LinearLayout>