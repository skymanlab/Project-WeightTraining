<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/item_margin_top"
    android:background="@color/colorBackgroundWhite"
    android:layout_marginBottom="@dimen/item_margin_bottom"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/custom_list_my_program_item_content_wrapper"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="?attr/selectableItemBackground"
        android:orientation="vertical"
        android:paddingStart="@dimen/item_padding_start"
        android:paddingTop="@dimen/item_padding_top"
        android:paddingEnd="@dimen/item_padding_end"
        android:paddingBottom="@dimen/item_padding_bottom">

        <TextView
            android:id="@+id/custom_list_my_program_item_program_nick_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/eland_choice_bold"
            android:text="@string/custom_list_my_program_item_program_nick_name"
            android:textColor="@color/colorTextBlack"
            android:textSize="@dimen/text_title_size" />

        <include
            layout="@layout/include_dot_divider"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/custom_list_my_program_item_all_muscle_area_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:fontFamily="@font/eland_choice_light"
                android:text="@string/custom_list_my_program_item_all_muscle_area_list"
                android:textColor="@color/colorTextBlack" />

            <View
                android:layout_width="1dp"
                android:layout_height="match_parent"
                android:layout_marginStart="@dimen/divider_vertical_layout_Start"
                android:layout_marginEnd="@dimen/divider_vertical_layout_End"
                android:background="@color/browser_actions_title_color" />

            <TextView
                android:id="@+id/custom_list_my_program_item_total_set_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/eland_choice_medium"
                android:text="총 13세트"
                android:textColor="@color/colorTextBlack" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>